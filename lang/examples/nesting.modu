fn example(a) {
    if a == 3 {
        print("Hello, World");

        if a - 1 - -1 == 3 {
            print("HELLO, WORLD");
        }
    }
}

fn test_nesting(a, b, c) {
    if a == b {
        print("a is equal to b");

        if c == 3 {
            example(c);
        }
    }

    if a != b {
        print("a is not equal to b");

        if c == 3 {
            example(c);
        }
    }
}

test_nesting(1, 1, 3)